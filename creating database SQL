CREATE TABLE user_data (
id INT NOT NULL AUTO_INCREMENT,
age FLOAT,
high_risk_exposure_occupation VARCHAR(5),
high_risk_interactions VARCHAR(5),
diabetes VARCHAR(5),
chd VARCHAR(5),
htn VARCHAR(5),
cancer VARCHAR(5),
asthma VARCHAR(5),
copd VARCHAR(5),
autoimmune_dis VARCHAR(5),
smoker VARCHAR(5),
temperature FLOAT,
pulse FLOAT,
labored_respiration VARCHAR(5),
cough VARCHAR(5),
fever VARCHAR(5),
sob VARCHAR(5),
diarrhea VARCHAR(5),
fatigue VARCHAR(5),
headache VARCHAR(5),
loss_of_smell VARCHAR(5),
loss_of_taste VARCHAR(5),
runny_nose VARCHAR(5),
muscle_sore VARCHAR(5),
sore_throat VARCHAR(5),
prediction VARCHAR(100),
PRIMARY KEY(id));
